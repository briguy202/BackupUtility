<Window x:Class="BackupUtility.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Backup Utility" Height="350" Width="525">
    <Grid>
		<TabControl Margin="12" Name="tabControl1">
			<TabItem Header="tabItem1" Name="tabItem1">
				<Grid />
			</TabItem>
		</TabControl>
	</Grid>
	<Window.Background>
		<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="#FFBABAF2" Offset="1" />
			<GradientStop Color="White" Offset="0.418" />
		</LinearGradientBrush>
	</Window.Background>
</Window>
